class Solution {
    public boolean validWordAbbreviation(String w,String a){
        int i=0,j=0;
        while(i<w.length()&&j<a.length()){
            if(Character.isLetter(a.charAt(j))){
                if(w.charAt(i++)!=a.charAt(j++)) return false;
            }else{
                if(a.charAt(j)=='0') return false;
                int n=0;
                while(j<a.length()&&Character.isDigit(a.charAt(j)))
                    n=n*10+(a.charAt(j++)-'0');
                i+=n;
            }
        }
        return i==w.length()&&j==a.length();
    }
}
